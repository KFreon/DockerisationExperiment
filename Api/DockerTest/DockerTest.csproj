    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>d3d43283-7de5-44e2-8991-4637b94d49df</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
